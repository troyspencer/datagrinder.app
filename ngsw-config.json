{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.*.js",
        "!/*.j",
        "!/*.min.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/logo.png"
      ]
    }
  },
  {
    "name": "lazy-assets",
    "installMode": "lazy",
    "updateMode": "lazy",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  },
  {
    "name": "fonts",
    "resources": {
      "urls": [
        "https://fonts.googleapis.com/icon?family=Material+Icons",
        "https://fonts.googleapis.com/css?family=Roboto",
        "https://ajax.googleapis.com/ajax/libs/hammerjs/2.0.8/hammer.min.js"
      ]
    }
  }],
  "navigationUrls": [
    "/**",           
    "!/**/*.*",      
    "!/**/*__*",     
    "!/**/*__*/**"
  ]
}