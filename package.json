{
  "name": "playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "dev_appserver.py server/gaefiles/main/app.yaml",
    "serve": "ng serve --open",
    "game-hosting": "cd server && game_serve.bat",
    "http": "cd server/gaefiles/main/static && http-server --gzip -p 8081",
    "serve-test": "yarn local && yarn http",
    "local": "ng build -c production-local && yarn ngsw-patch",
    "ngsw-deploy": "./node_modules/.bin/ngsw-config server/gaefiles/main/static/ ./ngsw-config.json  /static/",
    "dist": "ng build --prod && yarn ngsw-deploy",
    "proto": "cd protobuf && protoc_gen.bat",
    "deploy": "gcloud app deploy --project=keen-ascent-205301 server/gaefiles/main/app.yaml",
    "deploy-test": "gcloud app deploy --project=keen-ascent-205301 --no-promote server/gaefiles/main/app.yaml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "7.0.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/pwa": "^0.10.2",
    "@angular/router": "^7.0.0",
    "@angular/service-worker": "^7.0.0",
    "@types/google-protobuf": "^3.2.7",
    "core-js": "^2.5.4",
    "critical": "^1.3.4",
    "google-protobuf": "^3.6.1",
    "grpc-web-client": "^0.6.3",
    "rxjs": "^6.3.3",
    "ts-protoc-gen": "^0.7.7",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  }
}
