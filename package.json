{
  "name": "playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "cd server && dev.bat",
    "serve": "ng serve --open",
    "game-hosting": "cd server && game_serve.bat",
    "serve-test": "yarn local && cd server/gaefiles/main/static && http-server -p 8081",
    "local": "ng build -c production-local && yarn ngsw-patch",
    "ngsw-deploy": "./node_modules/.bin/ngsw-config server/gaefiles/main/static/ ./ngsw-config.json  /static/ && yarn ngsw-patch",
    "ngsw-patch": "grep -vwE \"sourceMappingURL\" server/gaefiles/main/static/ngsw-worker.js > server/gaefiles/main/static/ngsw-worker.js.tmp && mv server/gaefiles/main/static/ngsw-worker.js.tmp server/gaefiles/main/static/ngsw-worker.js ",
    "dist": "ng build --prod && yarn ngsw",
    "proto": "cd protobuf && protoc_gen.bat",
    "deploy": "gcloud app deploy --project=keen-ascent-205301 server/gaefiles/main/app.yaml",
    "deploy-test": "gcloud app deploy --project=keen-ascent-205301 --no-promote server/gaefiles/main/app.yaml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/cdk": "^6.4.0",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/core": "6.0.9",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/material": "^6.4.0",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.9",
    "@angular/service-worker": "6.0.9",
    "@types/google-protobuf": "^3.2.7",
    "core-js": "^2.5.4",
    "google-protobuf": "^3.5.0",
    "grpc-web-client": "^0.6.2",
    "rxjs": "^6.2.2",
    "ts-protoc-gen": "^0.7.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.0-rc.0",
    "@angular/cli": "^6.1.0-rc.0",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/language-service": "^6.0.9",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
